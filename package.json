{
  "name": "rpi-rgb-led-matrix-web-api",
  "version": "1.0.0",
  "description": "A HTTP-based API to run a LED-matrix with rpi-rgb-led-matrix",
  "dependencies": {
    "bootstrap": "5.3.1",
    "browserify": "17.0.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "js-sha256": "^0.10.1",
    "path": "0.12.7",
    "semaphore": "1.1.0",
    "servestatic": "0.0.0",
    "uuid": "9.0.1",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@types/browserify": "12.0.38",
    "@types/cors": "^2.8.14",
    "@types/express": "4.17.17",
    "@types/node": "20.6.0",
    "@types/semaphore": "1.1.1",
    "@types/uuid": "9.0.4",
    "typescript": "5.2.2"
  },
  "main": "app.js",
  "directories": {
    "bin": "dist/bin",
    "lib": "node_modules"
  },
  "scripts": {
    "clean": "rm -rf ./dist/bin",
    "build": "npm run clean && ./node_modules/typescript/bin/tsc -p node-app/tsconfig.json",
    "start": "npm run build && node ./dist/bin/app.js"
  },
  "author": "Marco",
  "license": "LGPL-3.0-or-later"
}
